{
  "name": "@atls-ui-parts/tooltip",
  "version": "1.1.0",
  "license": "BSD-3-Clause",
  "type": "module",
  "main": "./src/index.ts",
  "bin": {
    "generate-component": "plop --plopfile=./dist/generator.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn library build && cp -R ./src/templates ./dist/templates",
    "generate-component": "NODE_OPTIONS=\"$NODE_OPTIONS --import tsx\" plop --plopfile=./src/generator.ts",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@atls-ui-parts/condition": "workspace:*",
    "@atls-utils/use-float": "workspace:*",
    "@floating-ui/react": "0.27.8",
    "framer-motion": "12.23.22",
    "plop": "4.0.4"
  },
  "devDependencies": {
    "@atls-ui-parts/layout": "workspace:*",
    "@storybook/react": "8.6.12",
    "@types/react": "19.1.2",
    "@vanilla-extract/css": "1.17.1",
    "@vanilla-extract/dynamic": "2.1.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "storybook": "8.6.12",
    "tsx": "4.20.5"
  },
  "peerDependencies": {
    "@vanilla-extract/css": "*",
    "@vanilla-extract/dynamic": "*",
    "react": "*",
    "react-dom": "*"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  },
  "typecheckSkipLibCheck": true
}
